    "typescript": "^3.0.1",
    "why-did-you-update": "^0.1.1"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"npm run start\" \"node ./server\"",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "flow": "flow",
    "test": "node scripts/test.js --env=jsdom",
    "test:all": "npm test -- --all",
    "test:coverage": "npm test -- --coverage",
    "test:e2e": "npm test -- -c jest-e2e-config.json",
    "test:e2e:all": "npm test -- --all -c jest-e2e-config.json",
    "test:e2e:debug": "npm test -- --e2edebug -c jest-e2e-config.json",
    "test:ci": "npm-run-all -p -r dev test:ci2",
    "test:ci2": "wait-on http-get://localhost:3000 && npm run test:all && npm run test:e2e:all",
    "backstop:reference": "node ./node_modules/backstopjs/cli/index.js reference",
    "backstop:test": "node ./node_modules/backstopjs/cli/index.js test",
    "backstop:approve": "node ./node_modules/backstopjs/cli/index.js approve",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "proxy": "http://localhost:3001",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "verbose": false,
    "collectCoverageFrom": [
      "src/banking/**/*.{js,jsx,ts,tsx}",
      "!src/banking/*.{js,jsx,ts,tsx}",
      "src/news/**/*.{js,jsx,ts,tsx}",
      "src/reactplayground/fruitautocomplete/**/*.{js,jsx,ts,tsx}",
      "src/misc/commentfeed/**/*.{js,jsx,ts,tsx}",
      "src/shared/**/*.{js,jsx,ts,tsx}",
      "!src/*.{js,jsx,ts,tsx}",
      "!src/**/__e2e__/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs,ts,tsx}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs",
      "ts",
      "tsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "flow"
    ],
    "plugins": [
      "babel-plugin-styled-components"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
